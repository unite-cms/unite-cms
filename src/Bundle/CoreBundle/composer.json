{
    "name": "unite-cms/core-bundle",
    "type": "symfony-bundle",
    "description": "Core functionality for unite cms",
    "require": {
        "symfony/symfony": "^4.0",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6",
        "jms/serializer-bundle": "^2.3",
        "knplabs/knp-paginator-bundle": "^2.7",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/monolog-bundle": "^3.1",
        "syslogic/doctrine-json-functions": "^2.0",
        "twig/twig": "^2.4",
        "webonyx/graphql-php": "^0.11.5",
        "symfony/swiftmailer-bundle": "^3.1",
        "oro/doctrine-extensions": "^1.2",
        "symfony/expression-language": "^4.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/console": "^4.0"
    },
    "autoload": {
        "psr-4": { "UniteCMS\\CoreBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\app\\": "Tests/app/" }
    },
    "authors": [
        {
            "name": "Franz Wilding",
            "email": "franz@unite.co.at"
        },
        {
            "name": "Stefan Kamsker",
            "email": "stefan@unite.co.at"
        }
    ]
}
